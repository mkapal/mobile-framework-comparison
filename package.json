{
  "name": "mobile-framework-comparison",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "run-s build analyze",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "run-s \"lint --fix\"",
    "schema:ts": "json2ts -i src/schemas/frameworks.json -o src/types/frameworks.ts",
    "prettier": "prettier --write src/**/*.{js,json,ts,tsx}"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@rjsf/core": "^2.4.0",
    "@rjsf/material-ui": "^2.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "json-schema": "^0.2.5",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.164",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "ajv": "^6.12.6",
    "better-ajv-errors": "^0.6.7",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "husky": "^4.3.0",
    "json-schema-to-typescript": "^9.1.1",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "source-map-explorer": "^2.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "cross-env CI=true yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier",
      "eslint --fix"
    ],
    "src/**/*.{json,md}": [
      "prettier"
    ]
  }
}
