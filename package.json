{
  "name": "mobile-framework-comparison",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "run-s build analyze",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "run-s lint:js lint:css",
    "lint:fix": "run-s lint:js:fix lint:css:fix",
    "lint:js": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:js:fix": "run-s \"lint:js --fix\"",
    "lint:css": "stylelint \"src/**/*.{js,ts,tsx}\"",
    "lint:css:fix": "run-s \"lint:css --fix\"",
    "prettier": "prettier --write src/**/*.{js,json,ts,tsx}"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@rjsf/core": "^2.4.0",
    "@rjsf/material-ui": "^2.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "json-schema": "^0.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "ajv": "^6.12.4",
    "better-ajv-errors": "^0.6.7",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "source-map-explorer": "^2.5.0",
    "stylelint": "^13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "stylelint --fix"
    ]
  }
}
